		"postcss": "^8.4.40",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"semantic-release": "^24.0.0",
		"svelecte": "^3.17.3",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"svelte-heros-v2": "^0.10.11",
		"svelte-preprocess": "^5.1.4",
		"sveltekit-superforms": "1.6.1",
