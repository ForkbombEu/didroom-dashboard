{
	"name": "@didroom/dashboard",
	"author": {
		"name": "Puria Nafisi Azizi",
		"email": "puria@dyne.org"
	},
	"description": "Advanced electronic signature service based on Zenroom",
	"version": "1.4.0",
	"private": true,
	"engines": {
		"node": ">=17",
		"pnpm": ">=8"
	},
	"scripts": {
		"predev": "pnpm run definitions",
		"dev": "vite dev",
		"prebuild": "pnpm run definitions",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:ui": "playwright test --ui",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"types": "npx -y pocketbase-typegen --env --out src/lib/pocketbase/types.ts",
		"features": "node ./src/lib/features/generateFeaturesList.mjs",
		"roles": "node ./src/lib/rbac/generateRolesList.mjs",
		"schema": "node ./src/lib/pocketbase/schema/generateSchema.mjs",
		"definitions": "pnpm types && pnpm schema && pnpm features && pnpm roles",
		"serve": "pnpm build && pnpm preview --host"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.9.1",
		"@playwright/test": "^1.41.2",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^12.0.0",
		"@semantic-release/error": "^4.0.0",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^10.0.5",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^13.0.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/debug": "^4.1.12",
		"@types/eslint": "^8.56.0",
		"@types/lodash": "^4.17.1",
		"@types/node-forge": "^1.3.11",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.17",
		"debug": "^4.3.4",
		"dotenv": "^16.4.4",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.35",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"prettier-plugin-tailwindcss": "^0.5.1",
		"semantic-release": "^23.1.1",
		"svelecte": "^3.17.3",
		"svelte": "^4.2.11",
		"svelte-check": "^3.6.4",
		"svelte-heros-v2": "^0.10.11",
		"svelte-preprocess": "^5.1.3",
		"sveltekit-superforms": "1.6.1",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.3",
		"vitest": "^1.2.2",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.8",
		"@inlang/paraglide-sveltekit": "0.8.1",
		"@peculiar/x509": "^1.10.1",
		"@popperjs/core": "^2.11.8",
		"@sentry/sveltekit": "^7.66.0",
		"@slangroom/core": "^1.13.0",
		"@slangroom/qrcode": "^1.13.0",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@types/adm-zip": "^0.5.5",
		"adm-zip": "^0.5.10",
		"asn1js": "^3.0.5",
		"classnames": "^2.5.1",
		"clsx": "^2.0.0",
		"dayjs": "^1.11.9",
		"effect": "^3.1.2",
		"execa": "^8.0.1",
		"flowbite": "^2.3.0",
		"flowbite-svelte": "^0.44.23",
		"flowbite-svelte-blocks": "^1.1.0",
		"flowbite-svelte-icons": "^1.3.1",
		"json-schema-builder-svelte": "github:dyne/json-schema-builder-svelte",
		"lodash": "^4.17.21",
		"nanoid": "^5.0.2",
		"node-forge": "^1.3.1",
		"pocketbase": "^0.21.2",
		"slugify": "^1.6.6",
		"svelte-boring-avatars": "^1.2.5",
		"svelte-heros": "^6.0.1",
		"svelte-persisted-store": "^0.9.1",
		"tailwind-merge": "^2.3.0",
		"xml-formatter": "^3.5.0",
		"zenroom": "^4.12.0"
	}
}
