{
	"name": "@didroom/dashboard",
	"author": {
		"name": "Puria Nafisi Azizi",
		"email": "puria@dyne.org"
	},
	"description": "Advanced electronic signature service based on Zenroom",
	"version": "1.28.1",
	"private": true,
	"engines": {
		"node": ">=17",
		"pnpm": ">=8"
	},
	"scripts": {
		"predev": "pnpm run definitions",
		"dev": "vite dev",
		"prebuild": "pnpm run definitions",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:ui": "playwright test --ui",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"types": "npx -y pocketbase-typegen --env --out src/lib/pocketbase/types.ts",
		"features": "node ./src/lib/features/generateFeaturesList.mjs",
		"roles": "node ./src/lib/rbac/generateRolesList.mjs",
		"schema": "node ./src/lib/pocketbase/schema/generateSchema.mjs",
		"definitions": "pnpm types && pnpm schema && pnpm features && pnpm roles",
		"serve": "pnpm build && pnpm preview --host",
		"i18n:lint": "pnpm inlang lint --project ./project.inlang"
	},
	"devDependencies": {
		"@inlang/cli": "^2.18.1",
		"@inlang/paraglide-js": "1.11.2",
		"@playwright/test": "^1.45.3",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.0",
		"@semantic-release/error": "^4.0.0",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^10.1.3",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^14.0.1",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/debug": "^4.1.12",
		"@types/eslint": "^9.6.0",
		"@types/lodash": "^4.17.7",
		"@types/node-forge": "^1.3.11",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"autoprefixer": "^10.4.19",
		"debug": "^4.3.6",
